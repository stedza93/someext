{
  "name": "Sport Fanatic - New Tab page",
  "description": "Override the new tab page with a new one with Sport Fanatic",
  "version": "0.1",
  "incognito": "split",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "permissions": ["storage", "unlimitedStorage"],
  "content_security_policy":
    "script-src 'self' https://json.geoiplookup.io https://query.yahooapis.com; object-src 'self'",
  "manifest_version": 2
}
